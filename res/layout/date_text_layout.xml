	android:layout_width="wrap_content"
	android:layout_height="wrap_content"
	android:gravity="center" >
	
    <RelativeLayout 
        android:id="@+id/date_text_inner_inflate_relative"
		android:layout_width="wrap_content"
		android:layout_height="wrap_content" >
        
        <TextView 
			android:layout_width="wrap_content"
			android:layout_height="wrap_content"
			android:id="@+id/inflate_date_text"
			android:layout_centerInParent="true"/>
        
    </RelativeLayout>
    
</RelativeLayout>
